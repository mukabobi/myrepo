{
  "type": "object",
  "properties": {
    "isTracker": {
      "type": "identifier",
      "selector": "body",
      "control": "exists"
    },
    "trackerList": {
      "type": "array",
      "selectorAll": ".table tr",
      "items": {
        "type": "object",
        "properties": {
          "orderName": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText"
          },
          "status": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 1
          },
          "project": {
            "type": "string",
            "selector": "td",
            "control": "property.innerText",
            "selectIndex": 2
          }
        }
      }
    }
  },
  "connect": [
    {
      "match": [
        "isTracker"
      ],
      "screens": [
        "trackerlist"
      ]
    }
  ]
}
